{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "jsx": "preserve",
    "jsxImportSource": "solid-js",
    "baseUrl": "./",
    "paths": {
      "@components/*": ["src/components/*"],
      "@solidComponents/*": ["src/components/SolidJS/*"],
      "@componentsUtils/*": ["src/components/utils/*"],
      "@layouts/*": ["src/layouts/*"],
      "@styles/*": ["src/styles/*"]
    }
  }
}
