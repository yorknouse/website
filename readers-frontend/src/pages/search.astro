---
import SearchResults from "@components/SolidJS/SearchResults";
import Layout from "@layouts/Layout.astro";

const base = import.meta.env.PUBLIC_BASE_URL;
---
<Layout title="Welcome to Nouse" active={undefined} style={"nouse"}>
  <main class="px-[3%] pt-8 sm:px-[0.5%] 2xl:px-[13%]">
    <SearchResults client:only apiAddress={`${base}/api/frontend/searchSuggestions`} />
  </main>
</Layout>
